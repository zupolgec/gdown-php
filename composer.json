{
    "name": "zupolgec/gdown-php",
    "description": "Google Drive Public File Downloader when Curl/Wget Fails - PHP implementation",
    "type": "library",
    "keywords": ["google-drive", "downloader", "download", "curl", "wget"],
    "license": "MIT",
    "authors": [
        {
            "name": "Mattia Trapani",
            "email": "mattia.trapani@gmail.com"
        }
    ],
    "require": {
        "php": "^8.3",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^7.0",
        "psr/log": "^3.0",
        "symfony/console": "^6.0|^7.0",
        "symfony/css-selector": "^6.0|^7.0",
        "symfony/dom-crawler": "^6.0|^7.0"
    },
    "require-dev": {
        "pestphp/pest": "^3.0|^4.0"
    },
    "autoload": {
        "psr-4": {
            "Zupolgec\\GDown\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Zupolgec\\GDown\\Tests\\": "tests/"
        }
    },
    "bin": [
        "bin/gdown"
    ],
    "scripts": {
        "test": "pest",
        "lint": "mago lint",
        "analyze": "mago analyze",
        "fmt": "mago fmt",
        "fmt-check": "mago fmt --dry-run",
        "update-ua": "./scripts/update-user-agent.sh"
    },
    "config": {
        "sort-packages": true,
        "preferred-install": "dist",
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
